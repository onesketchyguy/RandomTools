<Window x:Class="ReplaceFiles.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ReplaceFiles"
        mc:Ignorable="d"
        Title="Replace files" Height="217.983" Width="243.476">
    <Grid Margin="0,0,2,0">
        <Button x:Name="CommitButton" Content="Commit" HorizontalAlignment="Left" Margin="10,157,0,0" VerticalAlignment="Top" Width="75" Click="CommitButton_Click" />
        <TextBlock x:Name="dirTextBlock" HorizontalAlignment="Left" Margin="10,13,0,0" TextWrapping="Wrap" Text="directory_text" VerticalAlignment="Top" />
        <Button x:Name="dirButton" Content="Pick directory" HorizontalAlignment="Left" Margin="10,34,0,0" VerticalAlignment="Top" Width="75" Click="dirButton_Click" />
        <TextBlock x:Name="copyObjectTextBlock" HorizontalAlignment="Left" Margin="10,65,0,0" TextWrapping="Wrap" Text="copy_file_text" VerticalAlignment="Top" />
        <Button x:Name="copyObjectButton" Content="Pick file" HorizontalAlignment="Left" Margin="10,86,0,0" VerticalAlignment="Top" Width="75" Click="copyObjectButton_Click" />
    </Grid>
</Window>